#!/bin/sh

#Add this to your autoexec
xrdb -merge ~/.Xresources
#wal -R
#nitrogen --restore
~/.fehbg
xinput --set-prop 13 "libinput Accel Speed" -1 
xrandr --output HDMI-0 --gamma 1.3:1.3:1.3
setxkbmap -layout "us,es" -option "grp:alt_shift_toggle"

picom &
lxpolkit &
#keepassxc &
sxhkd &
dunst &
#nm-applet &
#xfce4-clipman &
dwmblocks &
exec dwm 
